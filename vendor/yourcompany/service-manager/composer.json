{
    "name": "yourcompany/service-manager",
    "description": "Laravel package for managing online services with MongoDB",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "mongodb/laravel-mongodb": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "YourCompany\\ServiceManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourCompany\\ServiceManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YourCompany\\ServiceManager\\ServiceManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 