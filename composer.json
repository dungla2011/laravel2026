{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        { "type": "composer", "url": "https://packagist.org" },
        {
            "type": "path",
            "url": "./packages/servicemanager"
        },
        {
            "type": "path",
            "url": "./packages/mongocrud"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dungla2011/lad-lib-common.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dungla2011/lad-lib-laravel.git"
        }
    ],
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.263",
        "catfan/medoo": "^2.1",
        "claviska/simpleimage": "^4.0",
        "coldwinds/torrent-rw": "^1.1",
        "doctrine/dbal": "^3.10",
        "elasticsearch/elasticsearch": "^8.15",
        "ezyang/htmlpurifier": "^4.16",
        "facebook/webdriver": "*",
        "godruoyi/php-snowflake": "^3.2",
        "google/apiclient": "2.15.0",
        "google/cloud-text-to-speech": "^1.8",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^3.7",
        "kreait/firebase-php": "^7.0",
        "lad-lib/common": "dev-main",
        "lad-lib/laravel": "@dev",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "league/flysystem-ftp": "^3.29",
        "mongodb/laravel-mongodb": "^5.4",
        "mongodb/mongodb": "^1.12",
        "mpdf/mpdf": "^8.2",
        "nekland/tools": "^2.6",
        "phpmailer/phpmailer": "^6.7",
        "phpoffice/phpspreadsheet": "^3.3",
        "phpoffice/phpword": "^1.3",
        "picqer/php-barcode-generator": "^2.4",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.2",
        "ramsey/uuid": "^4.7",
        "sabre/dav": "^4.6",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-translatable": "^6.11",
        "stichoza/google-translate-php": "^5.3",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "thiagoalessio/tesseract_ocr": "^2.12",
        "tymon/jwt-auth": "^2.1",
        "workerman/workerman": "^4.2",
        "yourcompany/mongo-crud": "dev-main",
        "yourcompany/service-manager": "*",
        "zaloplatform/zalo-php-sdk": "^4.0"
    },
    "require-dev": {
        "andyabih/laravel-to-uml": "^1.0",
        "barryvdh/laravel-debugbar": "^3.10",
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^7.1",
        "laravel/dusk": "^8.3",
        "laravel/pint": "^1.15",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "8.6",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/common.php",
            "app/config.php",
            "app/giapha.php",
            "app/extra.php",
            "app/Components/simple_html_dom.php",
            "app/Helpers/i18n_helpers.php",
            "app/Helpers/locale_helpers.php",
            "app/Helpers/SequenceHelper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
