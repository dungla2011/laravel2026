        return null
    }

    //Tìm tất cả con cháu của obj
    findGetAllChildsDeepOfObj(obj) {
        let mmChild = []
        for (let any of this.dataPart) {
            if (this.checkObjIsChildOfOther(any, obj))
                mmChild.push(any)
        }
        return mmChild
