    checkNodeIsMainMan(obj) {
        let tmp = obj
        if (tmp.gender == 2 || tmp.married_with)
            return false
        let cc = 0
        //Kiểm tra tất cả các parent phải là MAN, thì sẽ là đinh
        while (tmp && tmp.parent_id > 0) {
            cc++
            if (cc > 10000)
                return false
            if (tmp.gender == 2 || tmp.married_with)
                return false
            tmp = this.getObjFromId(tmp.parent_id)
        }
        return true;
    }

