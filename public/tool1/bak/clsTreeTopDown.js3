            py = yChild
            strLine += px + "," + py

            px = xChild + wChild / 2
            py = yChild - this.spaceBetweenCellY / 2
            strLine += " " + px + "," + py

            px = xPar + wPar / 2
            py = yChild - this.spaceBetweenCellY / 2
            strLine += " " + px + "," + py

            px = xPar + wPar / 2
            py = yPar + hPar
            strLine += " " + px + "," + py

