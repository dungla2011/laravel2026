    //Tìm tất cả con cháu của obj và vợ chồng
    findGetAllChildsDeepOfObjAndMarried(obj) {
        let m1 = this.findGetAllChildsDeepOfObj(obj)
        if (this.findGetMariedOfObj(obj))
            m1.push(...this.findGetMariedOfObj(obj))
        return m1;
